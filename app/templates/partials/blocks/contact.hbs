<section class="contact wow" id="contact">
  <div class="container">
    <div class="contact__inner">
      <div class="contact__info">
        <div class="info__title">Office Info</div>
        <div class="info__desc"> <b>Office Hours:</b> Mon-Friday 8am - 10pm</div>
        <div class="info__desc"> <b> Address:</b> Igbalangao, Bugasong, Antique</div>
        <div class="info__phone"> <b>Tell:</b> 123-456-7890</div>
        <div class="info__phone"> <b>Fax:</b> 123-456-7890</div>
      </div>
      <div class="contact__form">
        <div class="contact__input">
          {{> ui/input  placeholder='Name' }}
          {{> ui/input  placeholder='Email' }}
        </div>

        {{> ui/input textarea=1 placeholder='Message' }}
        <button class="btn btn-secondary">Send Message</button>

      </div>
    </div>
    <div id="map">
      <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d9392.16769299017!2d27.59884675!3d53.94876315!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sru!2sby!4v1549526413034" width="100%" height="350" frameborder="0" style="border:0" allowfullscreen></iframe>
    </div>
  </div>

</section>
